generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model todo {
  id String @id
  title String 
  description String 
  status String

  @@map("todo_list_table")
}


model elias {
  id String @id
  name String
  phone_number String
  email String @unique

  @@map("student_info_table")
}